






<div class="row">
    <div class="box box-solid">
        <hr />
    </div>
</div>

<style>
    pre {
        font-family: 'Source Sans Pro', 'Helvetica Neue', Helvetica, Arial, sans-serif;
        border: none;
        white-space: pre-wrap;
        background-color: #f9fafc;
    }

    .clpre {
        background-color: #f9fafc;
        border: none;
        border-left: 1px solid #d2d6de;
    }


    option:empty {
        display: none;
    }

    .nNoo {
        margin-top: 5px;
        margin-bottom: 5px;
    }
</style>

<div class="row" ng-controller="EditController" id="ecc">


    <div class="col-md-4">
        
        <div class="box box-primary">
            <div class="box-header with-border">

                <div class="form-group box-title bg-purple" style="padding: 10px 20px 10px 10px; margin-top: -130px; margin-left: -10px; border-bottom-right-radius: 30px">
                    <h4 class="label no-padding"> SUPPLIER REGION DETAILS </h4>
                </div>

            </div>

            <div class="box-body bg-white">

                <div class="col-md-12">

                    <hr />

                    <div class="form-group">
                        <label>Supplier Id : </label>
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-user"></i></span>
                            <input type="text" class="form-control clpre" id="iddx" value="@ViewBag.User.idd" readonly>
                        </div>
                    </div>

                    <div class="form-group">
                        <label>Supplier Region : </label>
                        <div class="input-group">
                            <span class="input-group-addon" style="height: 30px"><i class="fa fa-user"></i></span>
                            <input type="text" class="form-control clpre" id="iddx" value="@ViewBag.User.region" readonly>
                        </div>
                    </div>

                    <hr />

                </div>
            </div>

        </div>

    </div>

    <div class="col-md-4">

        <div class="box box-primary">

            <div class="box-header with-border">

                <div class="form-group box-title bg-purple" style="padding: 10px 20px 10px 10px; margin-top: -130px; margin-left: -10px; border-bottom-right-radius: 30px">
                    <h4 class="label no-padding"> EMAIL & MOBILE DETAILS  </h4>
                </div>

            </div>

            <div class="box-body bg-white">

                <div class="col-md-12">

                    <hr />

                    <div class="form-group">
                        <label>Email Address : @ViewBag.User.email</label>
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-user"></i></span>
                            <input type="email" class="form-control" id="email" ng-model="ggInfo.Email" placeholder="Email Address" value="@ViewBag.User.email">                               
                        </div>
                    </div>

                    <div class="form-group">
                        <label>Mobile : @ViewBag.User.mobile</label>
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-user"></i></span>
                            <input type="text" class="form-control" id="mobile" ng-model="ggInfo.Mobile" placeholder="Mobile No." value="@ViewBag.User.mobile">
                        </div>
                    </div>
                    
                    <hr />

                    <a href="/sup/userregion/" class="btn btn-default btn-sm"> New Account Region</a>

                    <hr />

                </div>
            </div>
        </div>
    </div>
    
    <div class="col-md-4">

        <div class="box box-primary">

            <div class="box-header with-border">

                <div class="form-group box-title bg-purple" style="padding: 10px 20px 10px 10px; margin-top: -130px; margin-left: -10px; border-bottom-right-radius: 30px">
                    <h4 class="label no-padding"> PASSWORD DETAILS  </h4>
                </div>

            </div>

            <div class="box-body bg-white">

                <div class="col-md-12">

                    <hr />

                    <div class="form-group">
                        <label>Password : @ViewBag.User.password</label>
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-user"></i></span>
                            <input type="password" class="form-control" id="password" ng-model="ggform.password" placeholder="Password">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>Confirm Password : </label>
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-user"></i></span>
                            <input type="password" class="form-control" id="confirmPassword" ng-model="ggform.confirmPassword" placeholder="Confirm Password">
                        </div>
                    </div>

                    <hr />

                    <a href="/sup/userregion/" class="btn btn-default btn-sm"> New Account Region</a>

                    <hr />

                </div>
            </div>
        </div>
    </div>
    
</div>


