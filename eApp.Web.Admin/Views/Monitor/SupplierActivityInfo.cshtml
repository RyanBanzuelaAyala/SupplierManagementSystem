




<div class="row">
    <div class="box box-solid">
        <hr />
    </div>
</div>

<div class="box box-solid">
    <div class="box-body padding">
        <!-- left column -->
        <div class="col-md-6">
            <h3>Supplier Number : <b> @ViewBag.Info </b></h3>
            <p>below all the information of purchasing order(s).</p>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="box box-solid">

            <div class="box-body table-responsive padding">

                <div class="box-body">


                    <table id="example2" class="table table-bordered table-hover">

                        <thead>
                            <tr>
                                <th>
                                    Action
                                </th>
                                <th>
                                    Date / Time
                                </th>                               
                            </tr>
                        </thead>

                        <tbody class="poes">
                            @if(ViewBag.RequestQQ != null)
                            {
                                foreach(var req in ViewBag.RequestQQ)
                                {
                                    <tr>
                                        <td>
                                            @req.sysaction
                                        </td>
                                        <td>
                                            @req.sysdate
                                        </td>                                        
                                    </tr>
                                }
                            }

                        </tbody>

                    </table>
                </div>

            </div><!-- /.box-body -->
        </div>

    </div><!-- /.col -->


    <div class="col-xs-12">

    </div><!-- /.col -->
</div><!-- /.row -->
<!-- iCheck -->
<link href="~/Scripts/Javascript/plugins/iCheck/square/purple.css" rel="stylesheet" />
<!-- iCheck -->
<script src="~/Scripts/Javascript/plugins/iCheck/icheck.min.js"></script>
<!-- DataTables -->
<script src="~/Scripts/Javascript/plugins/datatables/jquery.dataTables.min.js"></script>
<script src="~/Scripts/Javascript/plugins/datatables/dataTables.bootstrap.min.js"></script>
<!-- DataTables -->
<link rel="stylesheet" href="~/Scripts/Javascript/plugins/datatables/dataTables.bootstrap.css">

<!-- page script -->
<script>
    $(function () {
        $('#example2').DataTable({
            "paging": true,
        });

    });
</script>


